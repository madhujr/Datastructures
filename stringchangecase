str1="hello World World Im Learning Python25"
str2="HELLO WORLD"
str3="hello"
str4="Hello World"
print(str1)
print(str2)
print(str3)
print(str4)
print(str.isupper(str1))
print(str1.upper())
print(str.islower(str1))
print(str.lower(str1))
print(str.swapcase(str2))
print(str.capitalize(str3))
print(str.istitle(str4))
print(str.title(str3))
